<% 
    showDashboardNews = (collection) =>{
        let iconData = {Contact: 'email',
                    Article: 'document',
                    Category: 'information',
                    RSS:      'social-rss'
        }
        let html =''
        collection.forEach((obj)=>{
            let icon = iconData[obj.name]
            html +=` <div class="col-lg-3 col-6">
                        <div class="small-box bg-info">
                            <div class="inner">
                                <h3>${obj.count}</h3>
                                <p>${obj.name}</p>
                            </div>
                            <div class="icon">
                                <i class="ion ion-${icon}"></i>
                            </div>
                            <a href="/${systemConfig.prefixAdmin}/${obj.slug}" class="small-box-footer">More info
                                <i class="fas fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>`
        })
        return html;
    }
%> 